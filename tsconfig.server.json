{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "baseUrl": "./",
    "module": "commonjs"
  },
  "exclude": [
    "**/test.ts",
    "**/*.spec.ts",
    "**/main.ts",
    "**/environment.*.ts"
  ],
  "angularCompilerOptions": {
    "entryModule": "src/server/app.server.module#AppServerModule",
    "enableIvy": "ngtsc"
  }
}
